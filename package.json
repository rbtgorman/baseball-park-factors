    "start": "npx @11ty/eleventy --serve"
  },
  "dependencies": {
    "@11ty/eleventy": "^2.0.1"
  },
  "devDependencies": {
    "netlify-cli": "^17.0.0"
  },
  "keywords": ["baseball", "park-factors", "eleventy", "netlify"],
